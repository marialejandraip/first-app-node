<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/product.css">
  </head>
  <body>
    <%- include('../includes/nav.ejs') %>
    <main>
      <% if(orders.length <=0 ) { %>
        <h1>Order</h1>
        <h2>Nothing there!</h2>
      <% } else { %>
        <ul>
          <%orders.forEach(order => { %>
          <li>
            <h1># <%= order._id %></h1>
            <ul>
              <% order.items.forEach(item => { %>
                <li><%= item.title %>(<%= item.quantity%>)</li>
              <% }); %>  
            </ul>
          </li>
          <% }); %>
        </ul>
      <% } %>
    </main>
<%- include('../includes/end.ejs') %>